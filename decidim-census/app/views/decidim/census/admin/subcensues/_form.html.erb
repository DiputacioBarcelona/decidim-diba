<div class="card">
  <div class="card-divider">
    <h2 class="card-title"><%= t(".title") %></h2>
  </div>

  <div class="card-section">
    <div class="row column">
      <%= form.text_field :name %>
    </div>
    <div class="row column">
      <%= form.select :decidim_participatory_process_id,
                      participatory_processes.map { |p| [translated_attribute(p.title), p.id] },
                      selected: form.object.try(:decidim_participatory_process_id),
                      include_blank: t(".select_participatory_process") %>
    </div>
    <div class="row column">
      <%= form.upload :subcensus_file %>
    </div>
 </div>

</div>
